{
  "expo": {
    "name": "FoodSaver",
    "slug": "foodsaver-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "foodsaver",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "web": {
      "bundler": "metro",
      "output": "single"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-web-browser",
      [
        "expo-camera",
        {
          "cameraPermission": "Allow FoodSaver to access your camera to scan barcodes."
        }
      ],
      [
        "expo-notifications",
        {
          "color": "#22c55e"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
